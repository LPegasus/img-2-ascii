{
  "name": "@lpegasus/image-to-ascii",
  "version": "1.0.1",
  "description": "Convert images to ASCII art",
  "keywords": [
    "ascii",
    "art",
    "cli",
    "image",
    "terminal",
    "console",
    "convert"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lpegasus/image-to-ascii"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "img2ascii": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "SKILL.md"
  ],
  "scripts": {
    "build": "tsup",
    "start": "tsx bin/cli.ts",
    "test": "node --test test/**/*.test.ts"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^14.0.3",
    "sharp": "^0.34.5"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "tsx": "^4.15.0",
    "tsup": "^8.0.0",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
